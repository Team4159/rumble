{
  "name": "@rumble/core",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "start": "ts-node -P tsconfig.json -r tsconfig-paths/register ./src/app.ts",
    "start:watch": "nodemon",
    "test": "jest src --coverage --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watchAll",
    "format": "prettier src",
    "format:fix": "npm run format -- --write",
    "format:check": "npm run format -- --check",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "lint:check": "npm run lint",
    "check": "npm run format:check && yarn run lint:check",
    "check:fix": "npm run format:fix && yarn run lint:fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.15",
    "redux-logger": "^3.0.6",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.8",
    "@types/redux-logger": "^3.0.8",
    "@types/socket.io": "^2.1.11",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  }
}
